{
  "name": "webapp",
  "version": "1.0.0",
  "description": "webapp",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier --write \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__/**/*.js\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t webapp .",
    "docker:run": "docker run -p 3000:3000 -d webapp",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop ecosystem.config.js --env production",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.2.1",
    "@loopback/context": "^1.11.0",
    "@loopback/core": "^1.4.1",
    "@loopback/openapi-v3": "^1.3.5",
    "@loopback/repository": "^1.4.1",
    "@loopback/rest": "^1.10.2",
    "@loopback/rest-explorer": "^1.1.16",
    "@loopback/service-proxy": "^1.1.4",
    "@types/dotenv": "^6.1.1",
    "@worldsibu/convector-platform-fabric": "^1.3.3",
    "@worldsibu/convector-storage-couchdb": "^1.3.3",
    "candidate-cc": "^0.1.0",
    "common-cc": "^0.1.0",
    "dotenv": "^7.0.0",
    "node-couchdb": "^1.3.0",
    "participant-cc": "^0.1.0",
    "pm2": "^3.5.0",
    "reflect-metadata": "^0.1.12",
    "training-cc": "^0.1.0",
    "trainingOffer-cc": "^0.1.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.4.3",
    "@loopback/testlab": "^1.2.5",
    "@loopback/tslint-config": "^2.0.4",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.2",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "ts-node": "^8.1.0",
    "tslint": "^5.15.0",
    "typescript": "^3.4.3"
  }
}
